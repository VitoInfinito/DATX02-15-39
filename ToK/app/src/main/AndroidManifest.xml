<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.kandidat.datx02_15_39.tok">

    <uses-permission android:name="android.permission.BLUETOOTH" />
    <uses-permission android:name="android.permission.BLUETOOTH_ADMIN" />
    <uses-permission android:name="android.permission.INTERNET" />

    <uses-feature
        android:name="android.hardware.bluetooth_le"
        android:required="false" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:targetSdkVersion="19"
        android:theme="@style/AppTheme">
        <activity
            android:name=".layout.MainActivity"
            android:label="@string/title_activity_hello_world"
            android:screenOrientation="portrait">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".layout.DietHomeActivity"
            android:label="@string/title_activity_diet_home"
            android:screenOrientation="portrait">
        </activity>
        <activity
            android:name=".layout.SleepHomeActivity"
            android:label="@string/title_activity_sleep_home"
            android:screenOrientation="portrait">
        </activity>
        <activity
            android:name=".layout.AddSleepActivity"
            android:label="@string/title_activity_add_sleep"
            android:parentActivityName=".layout.SleepHomeActivity"
            android:screenOrientation="portrait">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.kandidat.datx02_15_39.tok.layout.SleepHomeActivity" />
        </activity>
        <activity
            android:name=".layout.AddDietActivity"
            android:label="@string/title_activity_add_diet"
