            android:screenOrientation="portrait">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.kandidat.datx02_15_39.tok.layout.CustomActionBarActivity" />
        </activity>
        <activity
            android:name=".layout.GoalActivity"
            android:label="@string/title_activity_goal"
