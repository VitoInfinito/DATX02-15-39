            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.kandidat.datx02_15_39.tok.layout.MainActivity" />
        </activity>
        <activity
            android:name=".layout.WeightHomeActivity"
            android:label="@string/title_activity_weight_home"
            android:parentActivityName=".layout.MainActivity"
            android:screenOrientation="portrait">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.kandidat.datx02_15_39.tok.layout.MainActivity" />
        </activity>
        <activity
            android:name=".layout.CreateUserActivity"
            android:label="@string/title_activity_create_user"
            android:screenOrientation="portrait">
        </activity>
        /*Oauth activity*/
        <activity android:name=".jawbone.JawboneSetupActivity" >
            <intent-filter>
                <action android:name="com.kandidat.datx02_15_39.tok.jawbone.JawboneSetupActivity" />

                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>
        <activity android:name="com.jawbone.upplatformsdk.oauth.OauthWebViewActivity" >
            <intent-filter>
                <action android:name="com.jawbone.upplatformsdk.oauth.OauthWebViewActivity" />

                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>
        <activity android:name=".jawbone.UpApiListActivity" >
            <intent-filter>
                <action android:name="com.kandidat.datx02_15_39.tok.jawbone.UpApiListActivity" />

                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>
        <activity
            android:name=".layout.AccessoriesHomeActivity"
            android:label="@string/title_activity_accessories_home" >
            <intent-filter>
                <action android:name="com.kandidat.datx02_15_39.tok.layout.AccessoriesHomeActivity" />
                <category android:name="android.intent.category.DEFAULT" />
